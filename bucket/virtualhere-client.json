            "shortcuts": [
                [
                    "vhui64.exe",
                    "VirtualHere Client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ]
        },
        "32bit": {
            "url": "https://virtualhere.com/sites/default/files/usbclient/vhui32.exe",
            "hash": "5d672267833748ba9b45f14b17f9b7262db8d27c67eb40bc8f6fab4dec5e37e3",
            "bin": [
                [
                    "vhui32.exe",
                    "virtualhere-client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ],
            "shortcuts": [
                [
                    "vhui32.exe",
                    "VirtualHere Client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ]
        }
    },
    "pre_install": "$null = New-Item -Path $dir -Name vhui.ini -ItemType File",
    "persist": "vhui.ini",
    "checkver": {
        "regex": "Version ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://virtualhere.com/sites/default/files/usbclient/vhui64.exe"
            },
            "32bit": {
                "url": "https://virtualhere.com/sites/default/files/usbclient/vhui32.exe"
