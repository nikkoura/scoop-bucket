        "32bit": {
            "url": "https://virtualhere.com/sites/default/files/usbclient/vhui32.exe",
            "hash": "0a5499e2b2938320a7d9911b926f9651f27d25fbc0948e96ac22ec4c0a6be10d",
            "bin": [
                [
                    "vhui32.exe",
                    "virtualhere-client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ],
            "shortcuts": [
                [
                    "vhui32.exe",
                    "VirtualHere Client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ]
        }
    },
    "pre_install": "$null = New-Item -Path $dir -Name vhui.ini -ItemType File",
    "persist": "vhui.ini",
