    "license": {
        "identifier": "Proprietary",
        "url": "https://www.virtualhere.com/client_license"
    },
    "suggest": {
        "Server": "nikkoura/virtualhere-server"
    },
    "architecture": {
        "64bit": {
            "url": "https://virtualhere.com/sites/default/files/usbclient/vhui64.exe",
            "hash": "a2f7a0b9d8c82935d6aa8973d7efe500190bcdd46f00cba03842516914275ac9",
            "bin": [
                [
                    "vhui64.exe",
                    "virtualhere-client",
                    "-c $persist_dir\\vhui.ini"
