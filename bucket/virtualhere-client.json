            "shortcuts": [
                [
                    "vhui64.exe",
                    "VirtualHere Client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ]
        },
        "32bit": {
            "url": "https://virtualhere.com/sites/default/files/usbclient/vhui32.exe",
            "hash": "0da1d7fabadbd42b3d54f2a58cf2b76827ba6d53b9aa20fca7e5bfb05f6133ea",
            "bin": [
                [
                    "vhui32.exe",
                    "virtualhere-client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ],
            "shortcuts": [
                [
                    "vhui32.exe",
                    "VirtualHere Client",
                    "-c $persist_dir\\vhui.ini"
                ]
            ]
        }
    },
    "pre_install": "$null = New-Item -Path $dir -Name vhui.ini -ItemType File",
    "persist": "vhui.ini",
    "checkver": {
        "regex": "Version ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://virtualhere.com/sites/default/files/usbclient/vhui64.exe"
            },
            "32bit": {
                "url": "https://virtualhere.com/sites/default/files/usbclient/vhui32.exe"
